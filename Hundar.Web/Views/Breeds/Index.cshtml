@using Hundar.Web.Models
@model Breed[]
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html>
<head>
    <title>Breeds</title>
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/css/bootstrap.min.css" />
</head>
<body>
    <div class="container m-5 ">
        <h3 class="mb-1">Dogbreeds</h3>
        <div>
        <ul>
            @foreach (var breed in Model)
            {
                <li><div>@breed.BreedName</div></li>
            }
          
        </ul>
        </div>
        <h3><a asp-action="InsertBreed" >Add Breed</a></h3>
    </div>
    </body>
    </html>